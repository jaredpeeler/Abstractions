{
  "metadata": [
    {
      "src": [
        {
          "src": "../../",
          "files": [
            "AbstractionLayer/DirectScale.Disco.Extension.Abstractions/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appTitle": "DirectScale Abstractions",
      "_appFaviconPath": "images/favicon.ico",
      "_appLogoPath": "images/logo.svg",
      "_appFooter": "Copyright Â© 2021 <a href=\"https://directscale.com\">DirectScale</a><br>Generated by <strong>DocFX</strong>",
      "_disableContribution": true
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
     "templates/ds_temp"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}